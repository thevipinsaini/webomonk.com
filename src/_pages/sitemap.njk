---
pagination:
  data: collections.all
  size: 20
  alias: entries
layout: base.njk
title: "Sitemap"
permalink: /page-list/{% if pagination.pageNumber > 0 %}{{ pagination.pageNumber }}/{% endif %}
---


<div class="container">
	<h1>Sitemap</h1>

	<div>
		{% for entry in entries %}
			<p class="">
				<span class="font-weight-bold">
				{% if entry.data.title.length > 0 %}
					{{ entry.data.title }}
				{% else %}
					Missing Page Title
				{% endif %}
				</span>
				<a href="{{ entry.url }}"><code>{{ entry.url }}</code></a>
			</p>
		{% endfor %}
	</div><!-- list of pages -->
</div><!-- end container -->
